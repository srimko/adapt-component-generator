        "title": "<%= media2_title %>",
        "source": "<%= pathvideo %>/<%= media2_source %>",
        "type": "<%= media2_type %>"
      },
      {
        "title": "<%= media3_title %>",
        "source": "<%= pathvideo %>/<%= media3_source %>",
        "type": "<%= media3_type %>"
      },
      {
        "title": "<%= media4_title %>",
        "source": "<%= pathvideo %>/<%= media4_source %>",
        "type": "<%= media4_type %>"
      }
    ],
    "_playerOptions": {
        "poster": "",
